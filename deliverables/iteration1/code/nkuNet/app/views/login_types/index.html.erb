<p id="notice"><%= notice %></p>

<h1>Login Types</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @login_types.each do |login_type| %>
      <tr>
        <td><%= login_type.title %></td>
        <td><%= login_type.description %></td>
        <td><%= link_to 'Show', login_type %></td>
        <td><%= link_to 'Edit', edit_login_type_path(login_type) %></td>
        <td><%= link_to 'Destroy', login_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Login Type', new_login_type_path %>
