<p id="notice"><%= notice %></p>

<h1>Logins</h1>

<table>
  <thead>
    <tr>
      <th>Password</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Logintypes</th>
      <th>Active flag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logins.each do |login| %>
      <tr>
        <td><%= login.password %></td>
        <td><%= login.first_name %></td>
        <td><%= login.last_name %></td>
        <td><%= login.LoginTypes %></td>
        <td><%= login.active_flag %></td>
        <td><%= link_to 'Show', login %></td>
        <td><%= link_to 'Edit', edit_login_path(login) %></td>
        <td><%= link_to 'Destroy', login, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Login', new_login_path %>
